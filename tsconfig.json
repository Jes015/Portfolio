{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
    }
  },
  "extends": "astro/tsconfigs/strict"
}